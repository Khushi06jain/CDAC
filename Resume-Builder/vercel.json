{
    "buildCommand": "Resume-Builder/frontend && npm install && npm run build",
    "outputDirectory": "Resume-Builder/frontend/dist",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      }
    ],
    "functions": {
      "api/index.js": {
        "runtime": "nodejs14.x"
      }
    }
  }
  